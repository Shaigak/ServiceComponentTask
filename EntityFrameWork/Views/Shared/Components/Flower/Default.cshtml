@model IEnumerable<Flower>



     <div class="offset-lg-3 col-lg-6">
                      @foreach (var item in Model)
                      {
                            <div class="section-title">
                            <h1>@item.Name</h1>
                            <p class="text-black-50">@item.Description</p>
                        </div>
                      }
                    </div>
